@charset "utf-8";
/* CSS Document */
@font-face {
    font-family: ClementePDae-Light;
    src: url(images/clementepdae-light.ttf);
}

@media (min-width: 100px) {
	.myh3{
		font-size: small;
		text-align: center;
	}	
}

@media (min-width: 544px) {
	.myh3{
		font-size: small;
		text-align: center;
	}
}

/* Medium devices (tablets, 768px and up) The navbar toggle appears at this breakpoint */

@media (min-width: 768px) {
	.myh3{
		font-size: medium;
		text-align: center;
	}
}

/* Large devices (desktops, 992px and up) */

@media (min-width: 992px) {
	.myh3{
		font-size: x-large;
		text-align: center;
	}
}

/* Extra large devices (large desktops, 1200px and up) */

@media (min-width: 1200px) {
	.myh3{
		font-size: xx-large;
		text-align: center;
	}
}

body {
	background-color: #FFFFFF;
	color: #000000;
	font-family: Arial, sans-serif;
	font-size: 12pt;
	font-weight: normal;
	text-align: left;
}
a:link, a:active {
	font-size: 14pt;
	text-decoration: none;
	color: blue;

}
a:visited {
	text-decoration: none;
	color: green;
}

h1 {
	text-align: center;
	color: green;
	margin-top: 10px;
	padding-bottom: 0px;
	margin-bottom: 5px;
	font-size: 18pt;
	font-weight: normal;
}
h2 {
	padding-top: 10px;
	padding-bottom: 10px;
	font-weight: bold;
	font-size: 14pt;
	text-align: left;
}
h3 {
	padding-top: 10px;
	padding-bottom: 10px;
	font-weight: normal;
	font-size: 14pt;
	text-align: center;
	color: black;
}
img{
	float: bottom;
	display: block;
	margin-left: auto;
	margin-right: auto;
	width: 50%
}
.myh4 {
	float: bottom;
	text-align: center;
	padding-top: 10px;
	padding-bottom: 10px;
	font-size: 12pt;
}
.barra { 
    display: solid;
    width: 99%;
    height: 3px;
    background-color: #3300FF;
    border-color: #3300FF;
		margin-bottom: 8px;
		margin-top: 8px;

}

.regreso{
	float: right;
	padding-right: 20px;
}
